<div id="page-wrapper">
   <div id="page">
      <header id="header" class="header" role="banner">
         <div class="section layout-container clearfix">
            {{ page.secondary_menu }}
            {{ page.navbar }}
         </div>
      </header>
      {% if page.highlighted %}
      <div class="highlighted">
         <aside class="layout-container section clearfix" role="complementary">
            {{ page.highlighted }}
         </aside>
      </div>
      {% endif %}
      {% if page.featured_top %}
      <div class="featured-top">
         <aside class="featured-top__inner section layout-container clearfix" role="complementary">
            {{ page.featured_top }}
         </aside>
      </div>
      {% endif %}
      <div id="main-wrapper" class="layout-main-wrapper layout-container clearfix">
         <div id="main" class="layout-main clearfix">
            {% if page.sidebar_first %}
            <div id="sidebar-first" class="column sidebar">
               <aside class="section" role="complementary">
                  {{ page.sidebar_first }}
               </aside>
            </div>
            {% endif %}
            <div id="content" class="column main-content" role="main">
               {{ page.content }}
            </div>
            {% if page.sidebar_second %}
            <div id="sidebar-second" class="column sidebar" >
               <aside class="section" role="complementary">
                  {{ page.sidebar_second }}
               </aside>
            </div>
            {% endif %}
         </div>
      </div>
      {% if page.featured_bottom_first or page.featured_bottom_second or page.featured_bottom_third %}
      <div class="featured-bottom">
         <aside class="layout-container clearfix" role="complementary">
            {{ page.featured_bottom_first }}
            {{ page.featured_bottom_second }}
            {{ page.featured_bottom_third }}
         </aside>
      </div>
      {% endif %}

      <footer class="site-footer">
               {% if page.footer_first or page.footer_second or page.footer_third or page.footer_fourth or page.footer_fifth %}
               <div id= "footer-container" class="site-footer__top clearfix">

                  <div class="site-footer--logo">
                      <a href="https://stackoverflow-wd-io.ddev.site/">
                        <svg aria-hidden="true" class="native svg-icon iconLogoGlyphMd" width="32" height="37" viewBox="0 0 32 37">
                          <path d="M26 33v-9h4v13H0V24h4v9h22z" fill="#BCBBBB"></path>
                          <path d="M21.5 0l-2.7 2 9.9 13.3 2.7-2L21.5 0zM26 18.4L13.3 7.8l2.1-2.5 12.7 10.6-2.1 2.5zM9.1 15.2l15 7 1.4-3-15-7-1.4 3zm14 10.79l.68-2.95-16.1-3.35L7 23l16.1 2.99zM23 30H7v-3h16v3z" fill="#F48024"></path>
                        </svg>
                      </a>
                  </div>

                  <div id="footer_first">
                      {{ page.footer_first }}
                  </div>
                  <div id="footer_second">
                      {{ page.footer_second }}
                  </div>
                  <div id="footer_third">
                      {{ page.footer_third }}
                  </div>
                  <div id="footer_fourth">
                      {{ page.footer_fourth }}
                  </div>
                  <div id="footer_last">
                    <div id="footer_fifth">
                      {{ page.footer_fifth }}
                    </div>

                    <div id="copyright_string">
                      {{ page.copyright_string }}
                    </div>

                  </div>

               </div>
               {% endif %}

            </div>
         </div>
      </footer>
   </div>
</div>

